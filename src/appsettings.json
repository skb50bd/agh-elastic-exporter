{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Config": {
    "InstanceName": "docker1.of",
    "ElasticsearchEndpoints": [ "http://elasticsearch:9200" ],
    "ElasticsearchUsername": "agh_log_sender_system",
    "ElasticsearchPassword": "<Secure-Password>",
    "DefaultIndex": "agh-logs-*",
    "LogDirectory": "/var/logs/agh",
    "LogFilePattern": "querylog.json*",
    "CheckpointFile": "/app/checkpoints/checkpoint",
    "PollingInterval": "00:01:00",
    "AdGuardHomeConfigFile": "D:/AdGuardHome.yaml"
  }
}
